<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">



    <item
        android:gravity="left|top">
        <shape
            android:shape="rectangle">

            <corners android:topLeftRadius="10dp"
                android:bottomLeftRadius="10dp"
                android:topRightRadius="10dp"
                android:bottomRightRadius="10dp"/>

            <solid android:color="?attr/color_background_tertiary" />

            <size
                android:width="20dp"
                android:height="20dp" />

        </shape>
    </item>

    <item
        android:gravity="right|top">
        <shape
            android:shape="rectangle">

            <corners android:topLeftRadius="10dp"
                android:bottomLeftRadius="10dp"
                android:topRightRadius="10dp"
                android:bottomRightRadius="10dp"/>

            <solid android:color="?attr/color_background_tertiary" />

            <size
                android:width="20dp"
                android:height="20dp" />

        </shape>
    </item>

    <item
        android:gravity="bottom|left">
        <shape
            android:shape="rectangle">

            <corners android:topLeftRadius="10dp"
                android:bottomLeftRadius="10dp"
                android:topRightRadius="10dp"
                android:bottomRightRadius="10dp"/>

            <solid android:color="?attr/color_background_tertiary" />

            <size
                android:width="20dp"
                android:height="20dp" />

        </shape>
    </item>

    <item android:gravity="bottom|right">
        <shape
            android:shape="rectangle">

            <corners android:topLeftRadius="10dp"
                android:bottomLeftRadius="10dp"
                android:topRightRadius="10dp"
                android:bottomRightRadius="1dp"/>

            <solid android:color="?attr/color_background_tertiary" />

            <size
                android:width="20dp"
                android:height="20dp" />

        </shape>
    </item>

    <item>
        <shape
            android:shape="rectangle">
            <corners android:topLeftRadius="10dp"
                android:bottomLeftRadius="10dp"
                android:topRightRadius="10dp"
                android:bottomRightRadius="10dp"/>
            <solid android:color="?attr/color_background_tertiary" />
            <size
                android:width="45dp"
                android:height="45dp"/>
            <padding android:top="0dp"
                android:right="0dp"
                android:left="0dp"
                android:bottom="0dp"/>
        </shape>
    </item>
</layer-list>